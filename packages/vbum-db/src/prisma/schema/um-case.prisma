model um_case {
        id            Int      @id @default(autoincrement()) @db.UnsignedInt
        pub_id        String   @unique @db.VarChar(12)
        created_at    DateTime @db.DateTime(0)
        created_by    String   @db.VarChar(50)
        updated_at    DateTime @db.DateTime(0)
        updated_by    String   @db.VarChar(50)
        assigned_at   DateTime @db.DateTime(0)
        assigned_to   String   @db.VarChar(50)
        client_pub_id String   @db.VarChar(12)
        plan_pub_id   String   @db.VarChar(255)
        case_number   String   @db.VarChar(50)
        md_review     Boolean  @default(false)
        md_name       String   @db.VarChar(255)
        fu_action     String   @db.VarChar(255)
        p2p_success   Boolean  @default(false)
        status        String   @db.VarChar(255)
        remarks       String   @db.VarChar(1000)

        @@index([client_pub_id])
        @@index([plan_pub_id])
        @@index([assigned_to])
}

model um_case_hist {
        id            Int      @id @default(autoincrement()) @db.UnsignedInt
        hist_added_at DateTime @db.DateTime(0)
        pub_id        String   @db.VarChar(12)
        created_at    DateTime @db.DateTime(0)
        created_by    String   @db.VarChar(50)
        updated_at    DateTime @db.DateTime(0)
        updated_by    String   @db.VarChar(50)
        assigned_at   DateTime @db.DateTime(0)
        assigned_to   String   @db.VarChar(50)
        client_pub_id String   @db.VarChar(12)
        plan_pub_id   String   @db.VarChar(255)
        case_number   String   @db.VarChar(50)
        md_review     Boolean  @default(false)
        md_name       String   @db.VarChar(255)
        fu_action     String   @db.VarChar(255)
        p2p_success   Boolean  @default(false)
        status        String   @db.VarChar(255)
        remarks       String   @db.VarChar(1000)

        @@index([client_pub_id])
        @@index([plan_pub_id])
        @@index([assigned_to])
}
